{
	"name": "@rbxts/matter-hooks",
	"version": "0.1.2",
	"description": "A hooks library for Matter",
	"scripts": {
		"build": "rbxtsc",
		"watch": "rbxtsc -w",
		"prepublishOnly": "npm run build"
	},
	"keywords": [
		"roblox-ts",
		"roblox",
		"typescript",
		"matter",
		"hooks"
	],
	"repository": "github:lasttalon/matter-hooks-types",
	"homepage": "https://lasttalon.github.io/matter-hooks/",
	"bugs": "https://github.com/LastTalon/matter-hooks-types/issues",
	"author": "Lucas Gangstad <lucas@lasttalon.dev>",
	"license": "MIT",
